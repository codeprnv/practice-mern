   "bugs": {
      "url": "https://github.com/codeprnv/practice-mern/issues"
   },
   "homepage": "https://github.com/codeprnv/practice-mern#readme",
   "devDependencies": {
      "concurrently": "^9.2.1",
      "prettier": "^3.6.2",
      "prettier-plugin-tailwindcss": "^0.6.14"
   }
}
